{
  "name": "typescript-react-redux-boilerplate",
  "description": "React-Redux boilerplate with TypeScript and Webpack",
  "keywords": [
    "typescript",
    "react",
    "redux",
    "css modules",
    "boilerplate",
    "webpack"
  ],
  "version": "1.0.0",
  "author": "Krasimir Nedelchev (https://github.com/kaykayehnn)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kayKayEhnn/typescript-react-redux-boilerplate.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config=config/webpack.config.js --env=development",
    "build": "webpack --config=config/webpack.config.js --env=production --progress",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write \"src/**\"",
    "test": "jest --config=config/jest.config.js --passWithNoTests",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-starter-kit": "^0.5.1",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@svgr/webpack": "^4.2.0",
    "@testing-library/react": "^8.0.1",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.6",
    "@types/react-router-dom": "^4.3.2",
    "@types/webpack-env": "^1.13.8",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^2.0.1",
    "confusing-browser-globals": "^1.0.6",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "dts-css-modules-loader": "^1.0.1",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "^3.0.1",
    "fork-ts-checker-error-overlay-webpack-plugin": "0.0.1",
    "fork-ts-checker-webpack-plugin": "^1.2.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "husky": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "jest-watch-typeahead": "^0.3.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^7.0.1",
    "prettier": "^1.17.0",
    "react-dev-utils": "^9.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.4.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^4.2.0"
  },
  "browserslist": {
    "production": [
      "last 1 versions",
      "> 0.5%",
      "not ie > 0",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,ts,tsx}": [
        "prettier --write",
        "eslint --max-warnings=0",
        "git add"
      ],
      "*.{css,scss,md,html,json}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      ".*.js"
    ]
  }
}
