{
  "name": "typescript-react-redux-boilerplate",
  "description": "React-Redux boilerplate with TypeScript and Webpack",
  "keywords": [
    "typescript",
    "react",
    "redux",
    "css modules",
    "boilerplate",
    "webpack"
  ],
  "version": "1.0.0",
  "author": "Krasimir Nedelchev <kaykayehnn@outlook.com> (https://github.com/kaykayehnn)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kayKayEhnn/typescript-react-redux-boilerplate.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=config/tsconfig.json webpack-dev-server",
    "build": "cross-env TS_NODE_PROJECT=config/tsconfig.json webpack --env=production --progress",
    "lint": "eslint \"{src,__tests__}/**\"",
    "format": "prettier --write \"__tests__/**\", \"src/**\"",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^2.6.5",
    "normalize.css": "^8.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.4.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.2",
    "@types/classnames": "^2.2.7",
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/copy-webpack-plugin": "^4.4.3",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.9",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-mock-store": "^1.0.0",
    "@types/terser-webpack-plugin": "^1.2.1",
    "@types/webpack": "^4.4.25",
    "@types/webpack-dev-server": "^3.1.2",
    "@types/webpack-env": "^1.13.8",
    "@types/webpack-merge": "^4.1.3",
    "@types/workbox-webpack-plugin": "^3.6.1",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "autoprefixer": "^9.4.9",
    "awesome-typescript-loader": "^5.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^5.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "dts-css-modules-loader": "^1.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.4.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "react-dev-utils": "^7.0.3",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss,md,html,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
