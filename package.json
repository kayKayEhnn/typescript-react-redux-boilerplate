{
  "name": "typescript-react-redux-boilerplate",
  "description": "React-Redux boilerplate with TypeScript and Webpack",
  "keywords": [
    "typescript",
    "react",
    "redux",
    "css modules",
    "boilerplate",
    "webpack"
  ],
  "version": "1.0.0",
  "author": "Krasimir Nedelchev <kaykayehnn@outlook.com> (https://github.com/kaykayehnn)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kayKayEhnn/typescript-react-redux-boilerplate.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config=config/webpack.config.js --env=development",
    "build": "webpack --config=config/webpack.config.js --env=production --progress",
    "lint": "eslint \"{src,__tests__}/**\"",
    "format": "prettier --write \"__tests__/**\", \"src/**\"",
    "test": "jest --config=config/jest.config.js --passWithNoTests",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "classnames": "^2.2.6",
    "core-js": "^3.0.0",
    "normalize.css": "^8.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-starter-kit": "^0.4.3",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.9",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-mock-store": "^1.0.0",
    "@types/webpack-env": "^1.13.8",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "autoprefixer": "^9.4.9",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "dts-css-modules-loader": "^1.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.4.0",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "react-dev-utils": "^7.0.3",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss,md,html,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
