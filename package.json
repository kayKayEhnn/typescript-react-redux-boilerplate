{
  "name": "typescript-react-redux-boilerplate",
  "description": "React-Redux boilerplate with TypeScript and Webpack",
  "keywords": [
    "typescript",
    "react",
    "redux",
    "css modules",
    "boilerplate",
    "webpack"
  ],
  "version": "0.1.0",
  "author": "Krasimir Nedelchev <kaykayehnn@outlook.com> (https://github.com/kaykayehnn)",
  "repository": {
    "type": "git",
    "url": "https://github.com/kayKayEhnn/typescript-react-redux-boilerplate.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=config/tsconfig.json webpack-dev-server",
    "build": "cross-env TS_NODE_PROJECT=config/tsconfig.json webpack --env=production",
    "postbuild": "cp dist/index.html dist/404.html",
    "lint": "tslint --fix -p .",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "core-js": "^2.6.1",
    "normalize.css": "^8.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.4.0-beta.6",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.2",
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/copy-webpack-plugin": "^4.4.2",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/hard-source-webpack-plugin": "^1.0.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^23.3.11",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-mock-store": "^1.0.0",
    "@types/webpack": "^4.4.22",
    "@types/webpack-dev-server": "^3.1.1",
    "@types/webpack-env": "^1.13.6",
    "@types/webpack-merge": "^4.1.3",
    "autoprefixer": "^9.4.3",
    "awesome-typescript-loader": "^5.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "cssnano": "^4.1.8",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "react-dev-utils": "^7.0.0",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-standard": "^8.0.0",
    "typescript": "3.1.6",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "tslint --fix -c tslint.json",
      "jest --bail --findRelatedTests",
      "git add"
    ]
  }
}
